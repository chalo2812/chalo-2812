<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example"
    android:versionCode="1"
    android:versionName="1.0" xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/credi_grande"
        android:label="@string/Titulo"
        android:logo="@drawable/credi_grande"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.MainActivity"
            android:icon="@drawable/credicoop"
            android:label="@string/Titulo"
            android:logo="@drawable/credicoop" >
            <intent-filter
                android:icon="@drawable/credicoop"
                android:logo="@drawable/credicoop" >
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.cargar.InicioActivity" >
        </activity>
        <activity
            android:name="com.example.InicioActivity"
            android:label="@string/Titulo"
            android:parentActivityName="com.example.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.MainActivity" />
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>