var j: integer;

function FACTORIAL(N: integer) : integer;
var i, a: integer;
var tmp: integer;
begin
	
	if N > 3 then
		read tmp;
	end-if;
	
	a := 1;
	i := 1;
	
	while i <= N  do
    	a := a * i;
		i := i + 1;
    end-while;
    
end-func a;


procedure MAIN();
var X: integer;
begin
	showLN 'Calculo de todos los factoriales hasta el numero ingresado.';
	showLN '(Ej: 3!=3x2x1)';
	showLN ' ';
	
	show 'Ingresar numero:';
	read X;
	showLN ' ';
  
	j := 1;
	while j <= X do 
		showLN j, '!=', FACTORIAL(j);
		j := j + 1;
	end-while;
  
end-proc;
