function FIBONACCI(N: integer) : integer;
var actual, ant1, ant2, i: integer;
begin
  ant1 := 1;
  ant2 := 1;
  i := 2;
  if N < 1 then
    actual := 1;
  else
    while i <= N  do
    	actual := ant1 + ant2;
		ant2 := ant1;
		ant1 := actual;
		i := i + 1;
    end-while;
  end-if;
end-func actual;
 
procedure MAIN();
var X: integer;
begin
  showLN 'Este algoritmo calcula la sucesion de fibonacci para el numero ingresado';
  
  showLN 'La suma de la sucesion de fibonacci para ', 0, ' es ', FIBONACCI(0) + 1;
  showLN 'La suma de la sucesion de fibonacci para ', 1, ' es ', FIBONACCI(1) + 1;
  showLN 'La suma de la sucesion de fibonacci para ', 2, ' es ', FIBONACCI(2) + 1;
  showLN 'La suma de la sucesion de fibonacci para ', 3, ' es ', FIBONACCI(3) + 1;
  showLN 'La suma de la sucesion de fibonacci para ', 4, ' es ', FIBONACCI(4) + 1;
  
  
  showLN 'Ingresar numero: ';
  read X;
  showLN ' ';
  showLN 'La suma de la sucesion de fibonacci para ', X, ' es ', FIBONACCI(X) + 1;
end-proc;