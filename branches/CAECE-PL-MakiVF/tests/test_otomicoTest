"var X: integer; "  +
"var y: integer; "  +
"begin " +
"X := 3; "  +
"X := 6; "  +
"X := X - Y; "  +
"end-proc; " ;

"procedure MAIN(); "  +
"var X: integer; "  +
"var y: integer; "  +
"begin " +
"X := 3; "  +
"X := 6; "  +
"Y := X + Y; "  +
"end-proc; " ;


