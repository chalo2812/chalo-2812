
procedure MAIN();
var i: integer;
begin
	showLN 'Test funcional WHILE';
	
	i := 0;
	show 'Imprimo del 0 al 3:';
	while i <= 3  do
    	show ' ', i;
		i := i + 1;
    end-while;
    showLN ' ';
    
    i := 0;
	show 'Imprimo del 0 al 2:';
	while i < 3  do
    	show ' ', i;
		i := i + 1;
    end-while;
    showLN ' ';
    
    i:= 0;
	show 'Imprimo del 5 al 8:';
	while i <= 8  do
    	if i > 4 then
    		show ' ', i;
    	end-if;
    	
		i := i + 1;
    end-while;
	showLN ' ';

end-proc;
